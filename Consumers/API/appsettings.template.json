{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Main": "Host=${DB_HOST}; Database=ordermanager; Username=${DB_USER}; Password=${DB_PWD}"
  },
  "RabbitMQ": {
    "HostName": "${RMQ_HOST}",
    "Port": 5672,
    "UserName": "${RMQ_USER}",
    "Password": "${RMQ_PWD}"
  },
  "AllowedHosts": "*"
}